{
  "datasets": [
    {
      "name": "8f47f9fc",
      "displayName": "Monthly Taxi Revenue and Ride Analysis",
      "queryLines": [
        "SELECT\r\n",
        "    EXTRACT(MONTH FROM date) AS month,\r\n",
        "    EXTRACT(YEAR FROM date) AS year,\r\n",
        "    --SUM(sum_total_amount) AS sum_total_amount,\r\n",
        "    --SUM(total_rides) AS total_rides,\r\n",
        "    (SUM(sum_total_amount) / SUM(total_rides)) AS avg_total_amount\r\n",
        "FROM\r\n",
        "    workspace.default.nytaxis\r\n",
        "GROUP BY\r\n",
        "    month,\r\n",
        "    year\r\n",
        "ORDER BY\r\n",
        "    year,\r\n",
        "    month;"
      ]
    },
    {
      "name": "f9e7b79a",
      "displayName": "April Taxi Hourly Passenger Averages",
      "queryLines": [
        "SELECT\r\n",
        "    hour,\r\n",
        "    EXTRACT(MONTH FROM date) AS mes,\r\n",
        "    AVG(sum_passenger_count) AS avg_passenger_count_month\r\n",
        "FROM\r\n",
        "    (SELECT * FROM workspace.default.metrics_yellow\r\n",
        "    UNION ALL\r\n",
        "    SELECT * FROM workspace.default.metrics_green\r\n",
        "    )\r\n",
        "-- WHERE\r\n",
        "--     EXTRACT(MONTH FROM date) = 5\r\n",
        "GROUP BY\r\n",
        "    hour,mes\r\n",
        "ORDER BY\r\n",
        "    mes, hour;"
      ]
    },
    {
      "name": "549ef334",
      "displayName": "metrics_green",
      "queryLines": [
        "SELECT * FROM workspace.default.metrics_green"
      ]
    },
    {
      "name": "28ec62c0",
      "displayName": "metrics_yellow",
      "queryLines": [
        "SELECT * FROM workspace.default.metrics_yellow"
      ]
    }
  ],
